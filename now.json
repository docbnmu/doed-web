{
  "version": 2,
  "builds": [{ "src": "package.json", "use": "@now/static-build" }],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" },
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "/404" }
  ],
  "env": {
    "TZ": "America/Los_Angeles"
  },
   "regions": ["bom", "sin"]
}
